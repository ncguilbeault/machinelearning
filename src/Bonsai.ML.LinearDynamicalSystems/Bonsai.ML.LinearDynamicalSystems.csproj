<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Title>Bonsai.ML.LinearDynamicalSystems</Title>
    <Description>A Bonsai package for implementing the Kalman Filter using python.</Description>
    <Author>Nicholas Guilbeault</Author>
    <Copyright>Copyright Â©  2023</Copyright>
    <GeneratePackageOnBuild Condition="'$(Configuration)'=='Release'">true</GeneratePackageOnBuild>
    <PackageTags>Bonsai Rx ML KalmanFilter LinearDynamicalSystems</PackageTags>
    <TargetFrameworks>net472;netstandard2.0</TargetFrameworks>
    <Features>strict</Features>
    <Version>0.1.0</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Bonsai.Core" Version="2.8.1" />
    <PackageReference Include="Bonsai.Scripting.Python" Version="0.2.1" />
    <PackageReference Include="YamlDotNet" Version="13.7.1" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="*.py" />
    <EmbeddedResource Include="**\*.bonsai" />
    <EmbeddedResource Include="Bonsai.ML.LinearDynamicalSystems.svg" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Bonsai.ML\Bonsai.ML.csproj" />
  </ItemGroup>
</Project>